{
  "name": "hid0/mazer-laravel-ui",
  "description": "Laravel UI Admin Dashboard utilities from zuramai/mazer.",
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/hid0/mazer-laravel-ui",
  "support": {
    "issues": "https://github.com/hid0/mazer-laravel-ui/issues"
  },
  "keywords": [
    "laravel",
    "ui",
    "bootstrap5",
    "mazer",
    "admin-dashboard",
    "dashboard"
  ],
  "autoload": {
    "psr-4": {
      "Hid0\\MazerLaravelUi\\": "src/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Hid0\\MazerLaravelUi\\MazerServiceProvider"
      ]
    }
  },
  "authors": [
    {
      "name": "Faiz Hidayatulloh",
      "email": "faizhid11@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^7.1.3|^7.2.5|^7.3|^8.0|^8.1|^9.0",
    "laravel/ui": "^1.0|^2.0|^3.4",
    "illuminate/console": "~5.8|^6.0|^7.0|^8.0|^9.0",
    "illuminate/filesystem": "~5.8|^6.0|^7.0|^8.42|^9.0",
    "illuminate/support": "~5.8|^6.0|^7.0|^8.0|^9.0",
    "illuminate/validation": "^8.42|^9.0"
  },
  "require-dev": {
    "orchestra/testbench": "^6.23|^7.0"
  }
}
